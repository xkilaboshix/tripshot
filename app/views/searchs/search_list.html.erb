<div class="container">
  <h2>検索結果</h2>
  <div class="d-flex justify-content-center">
    <ul class="list-group list-group" style="max-width: 400px;">
      <% @lists.each do |list| %>
        <%= link_to list_path(list) do %>
          <li class="list-group-item list-group-item-primary">
            <%= attachment_image_tag list.user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg", size:'60x60' %>
            <%= list.title %>　出発日：<%= list.departure_date %><br>　　　　　　　帰宅日：<%= list.return_date %>
          </li>
        <% end %>
      <% end %>
    </ul>
  </div>
</div>